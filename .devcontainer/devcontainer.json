{
    "name": "openhexes",
    "dockerComposeFile": "compose.yml",
    "service": "app",
    "workspaceFolder": "/workspace",
    "remoteUser": "root",
    "forwardPorts": [
        5173,
        5432,
        8080
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                "dbaeumer.vscode-eslint",
                "esbenp.prettier-vscode",
                "zxh404.vscode-proto3",
                "golang.go",
                "ms-playwright.playwright",
                "MartyBeGood.single-editor-tabs",
                "bradlc.vscode-tailwindcss",
                "bufbuild.vscode-buf",
                "github.vscode-github-actions"
            ]
        }
    },
    "containerEnv": {
        "VITE_GOOGLE_CLIENT_ID": "${localEnv:HEXES_GOOGLE_CLIENT_ID}"
    },
    "features": {
        "ghcr.io/devcontainers/features/docker-in-docker": {
            "version": "latest",
            "dockerComposeVersion": "2"
        },
        "ghcr.io/nils-geistmann/devcontainers-features/zsh:0": {},
        "ghcr.io/joshuanianji/devcontainer-features/mount-pnpm-store:1": {},
        "ghcr.io/devcontainers/features/node:1": {
            "installYarnUsingApt": false
        },
        "ghcr.io/schlich/devcontainer-features/playwright:0": {},
        "ghcr.io/eitsupi/devcontainer-features/jq-likes:2": {},
        "ghcr.io/devcontainers/features/python:1": {
            "toolsToInstall": "pgcli"
        },
        "ghcr.io/devcontainers/features/go:1": {},
        "ghcr.io/mrsimonemms/devcontainers/buf:1": {},
        "ghcr.io/marcozac/devcontainer-features/atlas:1": {},
        "ghcr.io/devcontainers-extra/features/curl-apt-get:1": {},
        "ghcr.io/devcontainers-extra/features/upx:1": {},
        "ghcr.io/devcontainers-extra/features/fzf:1": {}
    }
}