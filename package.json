{
    "name": "ui",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "api": "cd /workspace/api && go run .",
        "build:api": "cd /workspace/api && GOFLAGS=-buildvcs=false go build -o /workspace/api/api . && pnpm upx",
        "build:ui": "pnpm install && tsc -b && vite build",
        "ci": "pnpm build:ui && pnpm lint && pnpm test",
        "codegen:migrations": "cd /workspace && atlas migrate diff --dir \"file://sqlc/migrations\" --to \"file://sqlc/schema.sql\" --dev-url \"postgres://$POSTGRES__USER:$POSTGRES__PASSWORD@$POSTGRES__HOSTNAME:$POSTGRES__PORT/tmp?sslmode=disable\"",
        "codegen:proto": "cd /workspace && buf lint && buf generate",
        "codegen:sql": "cd /workspace && pnpm sqlc vet && pnpm sqlc generate",
        "codegen": "pnpm codegen:sql && pnpm codegen:proto",
        "curl": "buf curl --schema /workspace/proto --protocol grpc --http2-prior-knowledge -H \"cookie: hexes.auth.google=${GOOGLE_COOKIE}\"",
        "db:migrate": "cd /workspace && atlas migrate apply -u \"postgresql://$POSTGRES__USER:$POSTGRES__PASSWORD@$POSTGRES__HOSTNAME:$POSTGRES__PORT/$POSTGRES__DB?sslmode=disable\" --dir \"file://sqlc/migrations/\"",
        "dev": "vite",
        "format": "prettier --write /workspace",
        "go:mod:tidy": "cd /workspace/api && go mod tidy",
        "launch": "pnpm build:ui && pnpm build:api && /workspace/api/api",
        "lint:api": "cd /workspace/api && go get && GOFLAGS=-buildvcs=false golangci-lint run --config /workspace/api/.golangci.yml .",
        "lint:ui": "eslint /workspace",
        "lint": "pnpm typecheck && pnpm lint:ui && pnpm lint:api",
        "pgcli": "pgcli --pgclirc /workspace/.devcontainer/pgcli.conf \"postgres://$POSTGRES__USER:$POSTGRES__PASSWORD@$POSTGRES__HOSTNAME:$POSTGRES__PORT/$POSTGRES__DB\"",
        "prepare": "husky",
        "psql": "docker run --rm -it postgres:17 psql \"postgres://$POSTGRES__USER:$POSTGRES__PASSWORD@$POSTGRES__HOSTNAME:$POSTGRES__PORT/$POSTGRES__DB\"",
        "sqlc": "docker run --rm -v $(pwd):/src -w /src sqlc/sqlc",
        "test:api": "cd /workspace/api && MODE=testing go test -v ./...",
        "test:ui": "pnpm exec playwright test",
        "test": "pnpm test:api && pnpm test:ui",
        "typecheck": "pnpm exec tsc -b --noEmit",
        "upx": "if ! upx -t /workspace/api/api 2>/dev/null; then upx --lzma -9 /workspace/api/api; fi"
    },
    "dependencies": {
        "@bufbuild/protobuf": "^2.6.0",
        "@connectrpc/connect": "^2.0.2",
        "@connectrpc/connect-web": "^2.0.2",
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/sortable": "^10.0.0",
        "@dnd-kit/utilities": "^3.2.2",
        "@radix-ui/react-accordion": "^1.2.11",
        "@radix-ui/react-avatar": "^1.1.10",
        "@radix-ui/react-checkbox": "^1.3.2",
        "@radix-ui/react-collapsible": "^1.1.11",
        "@radix-ui/react-dialog": "^1.1.14",
        "@radix-ui/react-dropdown-menu": "^2.1.15",
        "@radix-ui/react-hover-card": "^1.1.14",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-popover": "^1.1.14",
        "@radix-ui/react-progress": "^1.1.7",
        "@radix-ui/react-scroll-area": "^1.2.9",
        "@radix-ui/react-select": "^2.2.5",
        "@radix-ui/react-separator": "^1.1.7",
        "@radix-ui/react-slider": "^1.3.5",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-switch": "^1.2.5",
        "@radix-ui/react-tabs": "^1.1.12",
        "@radix-ui/react-tooltip": "^1.2.7",
        "@react-google-maps/api": "^2.20.7",
        "@react-oauth/google": "^0.12.2",
        "@tailwindcss/vite": "^4.1.11",
        "@tanstack/react-query": "^5.83.0",
        "@tanstack/react-query-devtools": "^5.83.0",
        "@tanstack/react-table": "^8.21.3",
        "@uidotdev/usehooks": "^2.4.1",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "cmdk": "^1.1.1",
        "codegenv2": "link:@bufbuild/protobuf/codegenv2",
        "dayjs": "^1.11.13",
        "framer-motion": "^12.23.12",
        "js-cookie": "^3.0.5",
        "lucide-react": "^0.525.0",
        "next-themes": "^0.4.6",
        "nuqs": "^2.4.3",
        "radix-ui": "^1.4.2",
        "react": "^19.1.0",
        "react-day-picker": "^9.8.0",
        "react-dom": "^19.1.0",
        "react-router": "^7.6.3",
        "sonner": "^2.0.6",
        "tailwind-merge": "^3.3.1",
        "tailwindcss": "^4.1.11",
        "use-local-storage-state": "^19.5.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.29.0",
        "@playwright/test": "^1.54.0",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/js-cookie": "^3.0.6",
        "@types/node": "^24.0.13",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "@vitejs/plugin-react": "^4.7.0",
        "babel-plugin-react-compiler": "19.1.0-rc.2",
        "eslint": "^9.29.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-react-hooks": "6.0.0-rc1",
        "eslint-plugin-react-refresh": "^0.4.20",
        "globals": "^16.2.0",
        "husky": "^9.1.7",
        "jiti": "^2.4.2",
        "prettier": "^3.6.2",
        "tw-animate-css": "^1.3.5",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.34.1",
        "vite": "^7.0.0"
    },
    "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
